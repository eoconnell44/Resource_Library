<table class="table table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th colspan="4">Title</th>
      <th colspan="4">Subject</th>
      <th colspan="4">Link</th>
      <th colspan="4">Date</th>
    </tr>
  </thead>
  <tbody>
    

  <% data.forEach(function(d) { %>
    <tr>
      <th scope="row" data-id='<%= d.id %>'> <%= d.id %> </th>
      <td colspan="4" type='text' name='title'> <%= d.title %> </td>
      <td colspan="4" type='text' name='subject'> <%= d.subject %> </td>
      <td colspan="4" type='text' name='title'> <%= d.link %> </td>
    </tr>
    <tr>
    <% }) %>
    </tbody>
</table>